@model List<Listing>@{    ViewBag.Title = "My Listings";}<h2>My Listings</h2><br /><a asp-controller="Listing" asp-action="Add">Add New Listing</a><br /><br /><table class="display-listings">    <thead>        <tr>            <th>ID</th>            <th>House Type</th>            <th>Total Area</th>            <th>Rental Price</th>            <th>Description</th>            <th>Status</th>        </tr>    <thead>    <tbody>        @foreach (var listing in Model)        {            <tr>                <td>@listing.ListingId</td>                <td>@listing.HouseType.Type</td>                <td>@listing.TotalArea</td>                <td>@listing.RentalPrice</td>                <td>@listing.Description</td>                <td>@(listing.Status ? "Active" : "Inactive")</td>                <td>                    <a asp-controller="Listing" asp-action="Details"                        asp-route-id="@listing.ListingId">Details</a>                    <a asp-controller="Listing" asp-action="Edit"                        asp-route-id="@listing.ListingId">Edit</a>                    <a asp-controller="Listing" asp-action="Delete"                        asp-route-id="@listing.ListingId">Delete</a>                </td>            </tr>        }    <tbody></table>