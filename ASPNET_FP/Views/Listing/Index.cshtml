@model List<Listing>@{    ViewBag.Title = "My Listings";}<h2>My Listings</h2><br /><a asp-controller="Listing" asp-action="Add">Add New Listing</a><br /><br /><table class="table table-striped">    <thead>        <tr>            <th scope="col">ID</th>            <th scope="col">House Type</th>            <th scope="col">Total Area</th>            <th scope="col">Rental Price</th>            <th scope="col"> Description</th>            <th scope="col">Status</th>            <th scope="col"></th>            <th scope="col"></th>                    </tr>    <thead>    <tbody>        @foreach (var listing in Model)        {            <tr>                <td scope="row">@listing.ListingId</td>                <td>@listing.HouseType.Type</td>                <td>@listing.TotalArea</td>                <td>@listing.RentalPrice</td>                <td>@listing.Description</td>                <td>@(listing.Status ? "Active" : "Inactive")</td>                <td>                    <a asp-controller="Listing" asp-action="Details"                        asp-route-id="@listing.ListingId">Details</a>                    <a asp-controller="Listing" asp-action="Edit"                        asp-route-id="@listing.ListingId">Edit</a>                    <a asp-controller="Listing" asp-action="Delete"                        asp-route-id="@listing.ListingId">Delete</a>                </td>            </tr>        }    <tbody></table>