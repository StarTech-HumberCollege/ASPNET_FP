@model List<Listing>@{    ViewBag.Title = "Listings";}<h2>All Listings</h2><a asp-controller="Listing" asp-action="Add">Add New Listing</a><table>    <thead>        <tr>            <th>ID</th>            <th>Total Area</th>            <th>Rental Price</th>            <th>Description</th>            <th>Status</th>        </tr>    <thead>    <tbody>        @foreach (var listing in Model)        {            <tr>                <td>@listing.ListingId</td>                <td>@listing.TotalArea</td>                <td>@listing.RentalPrice</td>                <td>@listing.Description</td>                <td>@(listing.Status ? "Active" : "Inactive")</td>                <td>                    <a asp-controller="Listing" asp-action="Details"                        asp-route-id="@listing.ListingId"                        asp-route-slug="@listing.Slug">Details</a>                    <a asp-controller="Listing" asp-action="Edit"                        asp-route-id="@listing.ListingId"                        asp-route-slug="@listing.Slug">Edit</a>                    <a asp-controller="Listing" asp-action="Delete"                        asp-route-id="@listing.ListingId"                        asp-route-slug="@listing.Slug">Delete</a>                </td>            </tr>        }    <tbody></table>